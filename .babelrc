{
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "targets": { "browsers": ["last 2 versions", "safari >= 7"] }
          }
        ],
        "stage-0",
        "react"
      ],
      "plugins": [
        [
          "import",
          [
            { "libraryName": "antd", "style": true },
            {
              "libraryName": "formsy-antd",
              "style": true,
              "libraryDirectory": "src"
            }
          ]
        ]
      ]
    },
    "production": {
      "presets": [
        "env",
        "stage-0",
        "react"
      ]
    }
  }
}
